+
+            if (tPlayer->duel && tPlayer->duel->opponent == GetMaster())
+                continue;
+
+            // heal
+            (HealTarget(*tPlayer, tPlayer->GetHealth() * 100 / tPlayer->GetMaxHealth()));
+        }
+    }
